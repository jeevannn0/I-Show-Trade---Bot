{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js", // Adjust this if your main entry point is different
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ]
  }
  